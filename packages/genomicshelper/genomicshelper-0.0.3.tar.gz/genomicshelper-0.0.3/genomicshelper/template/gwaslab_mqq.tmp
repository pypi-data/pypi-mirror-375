#!/bin/bash
## 
#################################
## NOTE #########################
## 
#################################
## ARGUMENTS ####################
## format = auto 
## build = "19" 
## verbose = True 
## skip = 2 
## cut = 20
## save = mymqq.png
## SCRIPT #####################

python3 <<EOF
import gwaslab as gl

mysumstats = gl.Sumstats("${sumstats}",
             fmt="${format}",
             build="${build}", 
             verbose=True)

mysumstats.plot_mqq(    skip = ${skip},
                        cut = ${cut},
                        anno="GENENAME",
                        build="${build}",
                        dpi=300,
                        save="${save}")

EOF