%content
<table class="table">
    !if columns:
        <thead>
            <tr>
                !for column in columns:
                    <th>{column}</th>
            </tr>
        </thead>
    <tbody>
        !for row in rows:
            <tr>
                !for cell in row:
                    <td>{cell}</td>
            </tr>
    </tbody>
</table>

