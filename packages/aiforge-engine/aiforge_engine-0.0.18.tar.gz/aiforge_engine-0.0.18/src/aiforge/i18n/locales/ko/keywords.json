    "required_count": {  
      "patterns": [  
        "(\\d+)(?:\\s+(?:items?|results?|entries?|records?))",  
        "(?:at most|maximum|max)\\s+(\\d+)",  
        "(?:first|top)\\s+(\\d+)",  
        "(?:at least|minimum|min)\\s+(\\d+)",  
        "process\\s+(\\d+)",  
        "generate\\s+(\\d+)",  
        "get\\s+(\\d+)",  
        "(\\d+)(?:\\s+(?:항목|결과|엔트리|레코드))",  
