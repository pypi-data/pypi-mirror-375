# AppLauncherAgent 配置文件（综合版）
# 这个文件包含AppLauncherAgent的特定配置

# 应用缓存大小
APP_LAUNCHER_AGENT_CACHE_SIZE=1000

# 调试模式
DEBUG_MODE=false

# 注册表扫描配置
# 是否扫描App Paths注册表
SCAN_APP_PATHS=true

# 是否扫描Uninstall注册表
SCAN_UNINSTALL_REGISTRY=true

# 是否扫描用户Uninstall注册表
SCAN_USER_UNINSTALL_REGISTRY=true

# 快捷方式扫描配置
# 是否扫描快捷方式
SCAN_SHORTCUTS=true

# 是否扫描开始菜单
SCAN_START_MENU=true

# 是否扫描桌面
SCAN_DESKTOP=true

# 最大扫描应用数量（防止过多应用影响性能）
MAX_APPS_SCAN=500 