#!HFB3!#

# Example of an HFB calculation for a deformed 240Pu nucleus.

action hfb

[action]
basisOptimization True
jobName           240Pu_deformed

[solver]
alternator/scheme WB

[interaction]
name              D1S

[basis]
nOscil            11
d_0               10.5
b_r               2.0
b_z               2.6
g_q               1.3

[constraints]
q10t              0.0
beta2t            1.4

[system]
nProt             94
nNeut             146
