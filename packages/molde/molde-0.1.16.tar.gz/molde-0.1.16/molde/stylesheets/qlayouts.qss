QFrame[frameShape="0"] {
    border: none;
}

QFrame[frameShape="1"],
QFrame[frameShape="2"] {
    border: 1px solid @border-color;
    padding: 0;
}

QWidget QFrame[frameShape="1"],
QWidget QFrame[frameShape="2"], 
QWidget QFrame[frameShape="6"] {
    border: 1px solid @border-color;
    border-radius: @border-radius;
    padding: 0;
}

QFrame::disabled {
    background: @background;
}


