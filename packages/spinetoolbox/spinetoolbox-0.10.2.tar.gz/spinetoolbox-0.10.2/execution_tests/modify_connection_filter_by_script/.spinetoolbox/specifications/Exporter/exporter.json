{
    "item_type": "Exporter",
    "output_format": "csv",
    "name": "Exporter",
    "description": "Exports all object parameter values to a single table.",
    "mappings": {
        "Object parameter values": {
            "type": "object_parameter_values",
            "mapping": [
                {
                    "map_type": "ObjectClass",
                    "position": "hidden"
                },
                {
                    "map_type": "ParameterDefinition",
                    "position": "hidden"
                },
                {
                    "map_type": "ParameterValueList",
                    "position": "hidden",
                    "ignorable": true
                },
                {
                    "map_type": "Object",
                    "position": "hidden"
                },
                {
                    "map_type": "Alternative",
                    "position": 0
                },
                {
                    "map_type": "ParameterValueType",
                    "position": "hidden"
                },
                {
                    "map_type": "ParameterValue",
                    "position": 1
                }
            ],
            "enabled": true,
            "always_export_header": true,
            "group_fn": "no_group",
            "use_fixed_table_name": false
        }
    }
}