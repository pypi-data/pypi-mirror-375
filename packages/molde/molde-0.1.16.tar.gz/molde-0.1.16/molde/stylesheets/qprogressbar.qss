QProgressBar {
    border-radius: 5px;
    text-align: center;
    background-color: @background-variant;
}
QProgressBar::chunk {
    background-color: @primary;
    border-top-right-radius: 3px;
    border-top-left-radius: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
}