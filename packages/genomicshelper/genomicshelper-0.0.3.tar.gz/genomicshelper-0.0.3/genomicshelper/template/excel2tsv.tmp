#!/bin/bash
## Convert EXCEL to tsv
#################################
## NOTE #########################
## pandas + openpyxl
#################################
## ARGUMENTS ####################
## excel : excel file path
## sheet=1 
## out=${excel}.tsv
#################################

python3 <<EOF
import pandas as pd 
df = pd.read_excel("${excel}",
                    sheet_name=${sheet},
                    dtype="string")
df.to_csv("${out}",sep="\t",index=None)
print("Converted sheet ${sheet} in ${excel} to ${out}.")
EOF

