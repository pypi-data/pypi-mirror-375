#!/bin/bash
## Convert EIGENSTRAT to PLINK PED
#################################
## NOTE #########################
##
#################################
## ARGUMENTS ####################
##  
#################################

echo \
'genotypename:    ${eigenstrat}.geno
snpname:         ${eigenstrat}.snp
indivname:       ${eigenstrat}.ind
outputformat:    PED
genotypeoutname: ${ped}.ped
snpoutname:      ${ped}.map
indivoutname:    ${ped}.pedind' > par.EIGENSTRAT.PED

convertf -p par.EIGENSTRAT.PED
