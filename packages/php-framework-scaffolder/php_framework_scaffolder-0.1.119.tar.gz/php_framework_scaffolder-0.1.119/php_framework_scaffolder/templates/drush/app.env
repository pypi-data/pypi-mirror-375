{% if database_type == "mysql" %}
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
{% endif %}
{% if database_type == "postgres" %}
DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
{% endif %}
DB_DATABASE={{ db_database }}
DB_USERNAME={{ db_username }}
DB_PASSWORD={{ db_password }}
{% for k, v in environment_variables.items() %}
{{ k }}={{ v }}
{% endfor %}
