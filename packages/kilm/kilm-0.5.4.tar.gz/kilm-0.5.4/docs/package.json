{
  "name": "kilm-docs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "sync-docs": "tsx scripts/sync-embedded-docs.ts sync",
    "sync-docs:watch": "tsx scripts/sync-embedded-docs.ts watch",
    "sync-docs:clean": "tsx scripts/sync-embedded-docs.ts clean",
    "dev": "concurrently \"pnpm sync-docs:watch\" \"astro dev\" --names \"sync,astro\" --prefix-colors \"blue,green\"",
    "start": "pnpm dev",
    "build": "pnpm sync-docs && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.35.2",
    "@astrojs/starlight-tailwind": "^4.0.1",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.13.5",
    "prettier": "^3.6.2",
    "starlight-site-graph": "^0.5.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "sharp": "^0.34.3",
    "tsx": "^4.20.5"
  },
  "pnpm": {
    "overrides": {
      "devalue": "^5.3.2",
      "tar-fs": "^3.1.0"
    }
  }
}
