<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="router_4G_service_contract_info_form_view" model="ir.ui.view">
    <field name="name">router.4G.service.contract.info.form</field>
    <field name="model">router.4g.service.contract.info</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <group>
            <field name="id_order"/>
            <field name="router_product_id" domain="[('categ_id', '=', %(router_category)d)]" options="{'no_create': True}"/>
            <field name="icc"/>
          </group>
          <group>
            <field name="vodafone_id"/>
            <field name="vodafone_offer_code"/>
          </group>
        </group>
      </form>
    </field>
  </record>
</odoo>
